<div class="container mt-5 pt-3">
  <h1><strong>Favorites</strong></h1>
  <% @favorites.each do |f| %>
    <div class="d-flex justify-content-between mt-5 my-4 card">
      <%= image_tag("logo.png", class: "uni-card-image logoo") %>
      <div>
      <br>
        <h3 class="title"><%= f.university.name %></h3>
        <p class="description">
        <%= f.university.description %></p>
      </div>
    </div>
  <% end %>
</div>
