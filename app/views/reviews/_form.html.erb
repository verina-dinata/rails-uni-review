
<%= simple_form_for([university, review],
        data: {
          insert_in_list_target: "form",
          action: "submit->insert-in-list#send"
        }) do |f| %>
  <%= f.input :comment %>
  <%= f.input :reputabilty_rating %>
  <%= f.input :education_quality_rating %>
  <%= f.input :campus_facilities_accom_rating %>
  <%= f.input :course_difficulty_rating %>
  <%= f.input :social_element_rating %>
  <%= f.input :value_for_money_rating %>
  <%= f.input :safety_rating %>
  <%= f.input :career_services_rating %>
  <%= f.button :submit %>
<% end %>
