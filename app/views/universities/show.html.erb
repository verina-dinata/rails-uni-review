<div class="banner">
  <div class="container p-3">
    <div class="row" margin="10px">
      <div class="col-md-9">
        <h1><%= @university.name %><button type="button" class="btn fav-button">‚ù§</button></h1>
        <h4><%= @university.city%>, <%= @university.country %></h4>
        <%= @university.reviews.length %> Reviews</p>
      </div>
      <div class="col-md-3">
        <img src="data:image/png;base64," alt="" height="80%" width="50%"><br>
      </div>
    </div>
    <div class="row">
      <div class="col-md-9">
        <h3>Ranked by our users: #<%= @university.ranking %></h2><br>
      </div>
      <div class="col-md-3">
        <button type="button" class="btn btn-primary">Visit Website</button>
      </div>
        <div class="row">
          <div class="accordion" id="accordionExample">
            <div class="accordion-item">
              <p class="accordion-header" id="headingOne">
                <button class="accordian-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                  Description
                </button>
              </p>
              <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
                <div class="accordion-body">
                  <p><%= @university.description %></p><br>
                </div>
              </div>
            </div>
          </div>
        </div>
    </div>
  </div>
  <div class="container">
    <button type="button" class="btn btn-primary">Book a campus tour</button><br>
    ----<br><br>

    <div class="row">
      <div id="map" class="col-md-7">

        <div class="" style="width: 100%; height: 600px;"
          data-controller="map"
          data-map-markers-value="<%= @markers.to_json %>"
          data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
        </div>

      </div>
      <div class="col-md-5">
        <strong><%= @university.name %></strong><br><br>
        <% @university.address.split(", ").each do |line|%>
          <%= "#{line}"%><br>
        <% end %>
      </div>
    </div>
  </div>
</div>